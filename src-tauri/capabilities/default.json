{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read",
    {
      "identifier": "fs:allow-read",
      "allow": [
        "$HOME/**",
        "$APPDATA/**",
        "$APPDATA_LOCAL/**",
        "$DESKTOP/**",
        "$DOCUMENTS/**",
        "$DOWNLOAD/**",
        "$PICTURES/**",
        "$VIDEOS/**",
        "$MUSIC/**"
      ]
    }
  ]
}

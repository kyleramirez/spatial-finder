{
  "extends": ["@tsconfig/svelte/tsconfig.json", "./tsconfig.json"],
  "compilerOptions": {
    "composite": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "paths": {
      "$lib/*": ["./src/lib/*"],
      "$types": ["./src/vite-env.d.ts"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.svelte", "src/vite-env.d.ts"],
}
